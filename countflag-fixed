[32m+[m[32mimport { getLocalStorage, getCartCount} from "./utils.mjs";[m
[32m+[m
[32m+[m[32mconst divTotal = document.querySelector(".cart-footer");[m
[32m+[m[32mconst displayTotal = document.querySelector(".cart-total");[m
[32m+[m
[32m+[m[32m//Total in Cart Feature[m
[32m+[m[32mfunction GetCartTotal() {[m
[32m+[m[32m    const cartItems = getLocalStorage("so-cart");[m
[32m+[m[32m    let total = 0.0;[m
[32m+[m
[32m+[m[32m    cartItems.forEach(item => {[m
[32m+[m[32m        total += parseFloat(item.FinalPrice * item.quantity);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    return total;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//Check if cart is empty[m
[32m+[m[32mif (getCartCount() === 0) {[m
[32m+[m[32m    divTotal.classList.toggle("hide");[m
[32m+[m[32m}[m
[32m+[m[32melse {[m
[32m+[m[32m    let subTotal = GetCartTotal();[m
[32m+[m[32m    divTotal.classList.toggle("cart-footer");[m
[32m+[m[32m    displayTotal.textContent += `$${subTotal.toFixed(2)}`;[m
[32m+[m[32m}[m
